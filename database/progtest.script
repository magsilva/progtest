CREATE MEMORY TABLE PUBLIC.USER(IDCODE INTEGER NOT NULL,USERNAME VARCHAR(50) NOT NULL,NAME VARCHAR(100),EMAIL VARCHAR(100),PASSWORD VARCHAR(80),ROLE VARCHAR(10) NOT NULL,CONSTRAINT PK_USER PRIMARY KEY(IDCODE),UNIQUE(USERNAME),UNIQUE(EMAIL))
CREATE MEMORY TABLE PUBLIC.COURSE(IDCODE INTEGER NOT NULL,NAME VARCHAR(50),STARTDATE DATE,ENDDATE DATE,INSTRUCTOR INTEGER NOT NULL,CONSTRAINT PK_COURSE PRIMARY KEY(IDCODE),CONSTRAINT FK_COURSE_INSTRUCTOR FOREIGN KEY(INSTRUCTOR) REFERENCES PUBLIC.USER(IDCODE) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE MEMORY TABLE PUBLIC.STUDENT_COURSE(STUDENT INTEGER NOT NULL,COURSE INTEGER NOT NULL,CONSTRAINT PK_STUDENT_COURSE PRIMARY KEY(STUDENT,COURSE),CONSTRAINT FK_STUDENT_COURSE_STUDENT FOREIGN KEY(STUDENT) REFERENCES PUBLIC.USER(IDCODE) ON DELETE CASCADE ON UPDATE CASCADE,CONSTRAINT FK_STUDENT_COURSE_COURSE FOREIGN KEY(COURSE) REFERENCES PUBLIC.COURSE(IDCODE) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE MEMORY TABLE PUBLIC.ASSIGNMENT(COURSE INTEGER NOT NULL,IDCODE INTEGER NOT NULL,TITLE VARCHAR(50),DESCRIPTION VARCHAR(500),STARTDATE TIMESTAMP,ENDDATE TIMESTAMP,PINSTTINST DOUBLE,CONSTRAINT PK_ASSIGNMENT PRIMARY KEY(COURSE,IDCODE),CONSTRAINT FK_ASSIGNMENT_COURSE FOREIGN KEY(COURSE) REFERENCES PUBLIC.COURSE(IDCODE) ON DELETE CASCADE ON UPDATE CASCADE)
CREATE MEMORY TABLE PUBLIC.ORACLE(IDCODE INTEGER NOT NULL,TITLE VARCHAR(50),DESCRIPTION VARCHAR(500),LANGUAGE VARCHAR(50),CONSTRAINT PK_ORACLE PRIMARY KEY(IDCODE))
CREATE MEMORY TABLE PUBLIC.TOOL(IDCODE INTEGER NOT NULL,NAME VARCHAR(50),TYPE VARCHAR(50),LANGUAGE VARCHAR(50),CONSTRAINT PK_TOOL PRIMARY KEY(IDCODE))
CREATE MEMORY TABLE PUBLIC.CRITERION(TOOL INTEGER NOT NULL,IDCODE INTEGER NOT NULL,NAME VARCHAR(50),CONSTRAINT PK_CRITERION PRIMARY KEY(TOOL,IDCODE),CONSTRAINT FK_CRITERION_TOOL FOREIGN KEY(TOOL) REFERENCES PUBLIC.TOOL(IDCODE))
CREATE MEMORY TABLE PUBLIC.ASSIGNMENT_CRITERION(COURSE INTEGER NOT NULL,ASSIGNMENT INTEGER NOT NULL,TOOL INTEGER NOT NULL,CRITERION INTEGER NOT NULL,WEIGHT DOUBLE,CONSTRAINT PK_ASSIGNMENT_CRITERION PRIMARY KEY(COURSE,ASSIGNMENT,TOOL,CRITERION),CONSTRAINT FK_ASSIGNMENT_CRITERION_ASSIGNMENT FOREIGN KEY(COURSE,ASSIGNMENT) REFERENCES PUBLIC.ASSIGNMENT(COURSE,IDCODE),CONSTRAINT FK_ASSIGNMENT_CRITERION_CRITERION FOREIGN KEY(TOOL,CRITERION) REFERENCES PUBLIC.CRITERION(TOOL,IDCODE))
CREATE MEMORY TABLE PUBLIC.EVALUATION(STUDENT INTEGER NOT NULL,COURSE INTEGER NOT NULL,ASSIGNMENT INTEGER NOT NULL,SUBMISSIONDATE TIMESTAMP,PSTTST DOUBLE,PINSTTST DOUBLE,PSTTINST DOUBLE,SCORE DOUBLE,CONSTRAINT PK_EVALUATION PRIMARY KEY(STUDENT,COURSE,ASSIGNMENT),CONSTRAINT FK_EVALUATION_STUDENT FOREIGN KEY(STUDENT) REFERENCES PUBLIC.USER(IDCODE) ON DELETE CASCADE ON UPDATE CASCADE,CONSTRAINT FK_EVALUATION_ASSIGNMENT FOREIGN KEY(COURSE,ASSIGNMENT) REFERENCES PUBLIC.ASSIGNMENT(COURSE,IDCODE) ON DELETE CASCADE ON UPDATE CASCADE)
